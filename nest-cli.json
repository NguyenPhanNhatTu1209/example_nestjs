{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/final-project/src",
  "monorepo": true,
  "root": "apps/final-project",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/final-project/tsconfig.app.json"
  },
  "projects": {
    "client": {
      "type": "application",
      "root": "apps/client",
      "entryFile": "main",
      "sourceRoot": "apps/client/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client/tsconfig.app.json"
      }
    },
    "authentication": {
      "type": "application",
      "root": "apps/authentication",
      "entryFile": "main",
      "sourceRoot": "apps/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication/tsconfig.app.json"
      }
    }
  }
}